{
  "name": "19-09-22-crud-growdevers",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/main/server.ts",
    "build": "npx tsc",
    "start": "node dist/main/server.js",
    "migration:show": "yarn typeorm-ts-node-commonjs migration:show -d src/main/database/typeorm.ts",
    "migration:run": "yarn typeorm-ts-node-commonjs migration:run -d src/main/database/typeorm.ts",
    "migration:create": "yarn typeorm migration:create src/main/database/migrations/CreateTableAssessments",
    "migration:revert": "yarn typeorm-ts-node-commonjs migration:revert -d src/main/database/typeorm.ts",
    "migration:test": "cross-env NODE_ENV=test yarn typeorm-ts-node-commonjs migration:run -d src/main/database/typeorm.ts",
    "test:setup": "rimraf test.sqlite && npm run migration:test",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:i": "npm run test:setup && cross-env NODE_ENV=test jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.16",
    "@types/pg": "^8.6.5",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "ioredis": "^5.2.4",
    "jest": "^29.3.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/supertest": "^2.0.12",
    "sqlite3": "^5.1.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3"
  }
}
